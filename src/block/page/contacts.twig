<main class="{{content_prefix}}panel  {{content_mod}}">
	<section class="{{content_prefix}}container container  {{content_mod}}">
		{% include "../common/breadcrumb/tpl.twig" with {
			prefix:prefix.breadcrumb, 
			mod:bread_mod, 
			bread:bread
		} %}
