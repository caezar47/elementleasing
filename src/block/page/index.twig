{% if view == "index_header" %}
	<section class="{{content_prefix}}panel  {{content_mod}}">
		<div class="{{content_prefix}}container container  {{content_mod}}">	
			<div class="{{content_prefix}}slider  {{content_mod}}" data-slick-header>
				{# <div class="slick-count"></div> #}
				{% for value in list %}
					<div class="{{content_prefix}}slider-slide">
						<a href="{{value.link}}" class="{{prefix.card}}item  {{content_mod}}">
							<div class="{{prefix.card}}wrap  {{content_mod}}">
								<p class="{{prefix.card}}preview  {{content_mod}}">
									<img src="{{value.preview}}" alt="" class="img-responsive">
								</p>
								<div>
									<p class="{{prefix.card}}heading  {{content_mod}}">{{value.name}}</p>
									<div class="{{prefix.card}}note  {{content_mod}}">{{value.note}}</div>
								</div>
							</div>
						</a>
					</div>
				{% endfor %}
			</div>
			{% include "../common/social/tpl.twig" with {
				prefix:"social__",
				group:"is--index-header", 
				mod:"is--index-header", 
				list:[
					{name:"soc-vk", link:soc.vk},
					{name:"soc-inst", link:soc.inst},
					{name:"soc-fb", link:soc.fb}
				]} 
			%}
		</div>
	</section>
{% endif %}
