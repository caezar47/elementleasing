			<div class="{{content_prefix}}row  grid__row  {{content_mod}}">
				<div>
					{% include "../common/page-header/tpl.twig" with {
						prefix:"page-header__", 
						mod:heading_mod,
						level:heading_level, 
						heading:heading
					} %}
					{% include "../common/page-header/tpl.twig" with {
						prefix:"page-header__", 
						mod:heading_n_mod,
						level:heading_n_level, 
						heading:heading_n
					} %}
					<a href="#" class="btn__item  is--arrow  is--md  is--success">Отправить заявку</a>
				</div>
				<div >
					{% include "../common/page-header/tpl.twig" with {
						prefix:"page-header__", 
						mod:heading_v_mod,
						level:heading_v_level, 
						heading:heading_v
					} %}
				</div>
			</div>
		</div>
	</section>
{% endif %}

{% if view == "index_products" %}
	<section class="{{content_prefix}}panel  {{content_mod}}">
		<div class="{{content_prefix}}container container  {{content_mod}}">
			{% include "../common/page-header/tpl.twig" with {
				prefix:"page-header__", 
				mod:heading_mod,
				level:heading_level, 
				heading:heading
			} %}
			<div class="{{content_prefix}}row  grid__row  {{content_mod}}">
				<div class="{{prefix}}preview  {{content_mod}}">
					<img src="{{preview}}" class="img-responsive" alt="">
				</div>
				{% if list %}
				<div class="panel-group" id="{{list_group}}" role="tablist" aria-multiselectable="true">
					{% for value in list %}
					<div class="{{prefix}}item  {{content_mod}}  panel  {% if value.status == "active" %}is--open{% endif %}">
						<div class="{{prefix}}heading  {{content_mod}}" role="tab" id="{{value.h_id}}">
							<a role="button" data-toggle="collapse" data-parent="#{{list_group}}" href="#{{value.id}}" {% if value.status == "active" %}aria-expanded="true"{% endif %} aria-controls="{{value.id}}">
								<p class="{{prefix}}icon  {{content_mod}}">
									{% include "../common/svg-sprite/svg.twig" with {icon:value.icon} %} 
								</p>
								<span>{{value.name}}</span>
							</a>
						</div>
						<div id="{{value.id}}" class="panel-collapse collapse {% if value.status == "active" %}in{% endif %}" role="tabpanel" aria-labelledby="{{value.h_id}}" {% if value.status == "active" %}aria-expanded="true"{% endif %}>
							<div class="{{prefix}}wrap {% if value.form == "true" %}	grid__row{% endif %}  {{content_mod}}">
								<div class="text__block  {{content_mod}}">
							        {{value.note}}
							    </div>
							    <a class="link__item  is--next  is--white-success" href="{{value.link}}">Подробнее</a>	 					    
						    </div>
						</div>
					</div>
					{% endfor %}
				</div>
				{% endif %}
			</div>
		</div>
	</section>
{% endif %}