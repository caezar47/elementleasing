{% if view == "leasing_seo" %}
	<section class="{{content_prefix}}panel  {{content_mod}}">
		<div class="{{content_prefix}}container container  {{content_mod}}">
			<div class="panel-group" id="{{list_group}}" role="tablist" aria-multiselectable="true">
				{% for value in list %}
				<div class="{{prefix}}item  {{mod}}  panel">
					<div class="{{prefix}}heading  {{mod}}" role="tab" id="{{value.h_id}}">
						<a role="button" data-toggle="collapse" data-parent="#{{list_group}}" href="#{{value.id}}" {% if value.status == "active" %}aria-expanded="true"{% endif %} aria-controls="{{value.id}}">
							{{value.name}}
						</a>
					</div>
					<div id="{{value.id}}" class="panel-collapse collapse {% if value.status == "active" %}in{% endif %}" role="tabpanel" aria-labelledby="{{value.h_id}}">
						<div class="{{prefix}}wrap {% if value.form == "true" %}	grid__row{% endif %}  {{mod}}">
							<div class="text__block  {{mod}}">
						        {{value.note}}
						    </div>
						    {% if value.form == "true" %}					    
								{% include "../../common/form/form.twig" with {
									view: 			"leasing_base", 
									prefix:			"form__", 
									mod:			"is--leasing-base", 
									color:			"is--primary-light", 
									ID: 			"f_leasing-base-seo", 
									heading:		"Запрос на лизинг", 
									heading_level:	"h3", 
									heading_mod:	"is--h3  fw--700  is--form-leasing-base",
									heading_sm:		"Оставьте заявку, и наш консультант перезвонит!", 
									heading_sm_level:"p", 
									heading_sm_mod:	"is--h6  fw--100  is--form-leasing-base-xs",
									heading_r:		"Спасибо!<br> Заявка отправлена", 
									heading_r_level:"h3", 
									heading_r_mod:	"is--h2 fw--700  is--form-leasing-base-result",
									heading_r_sm:	"В ближайшее время наш консультант перезвонит вам и ответит на вопросы.", 
									heading_r_sm_level:	"p", 
									heading_r_sm_mod:	"is--h6  fw--100"
								}%}
							{% endif %}
					    </div>
					</div>
				</div>
				{% endfor %}
			</div>
		</div>
	</section>
{% endif %}

