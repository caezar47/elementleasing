{% if view == "leasing_header" %}
	<section class="{{content_prefix}}panel  {{content_mod}}" {% if bg %}style="background-image: url({{bg}});"{% endif %}>
		<div class="{{content_prefix}}container container  {{content_mod}}">
			{% include "../../common/breadcrumb/tpl.twig" with {
				prefix:prefix.breadcrumb, 
				mod:bread_mod, 
				bread:bread
			} %}			
			{% include "../../common/page-header/tpl.twig" with {
				prefix:"page-header__", 
				mod:heading_mod,
				level:heading_level, 
				heading:heading
			} %}			
			{% include "../../common/page-header/tpl.twig" with {
				prefix:"page-header__", 
				mod:heading_sm_mod,
				level:heading_sm_level, 
				heading:heading_sm
			} %}		
			{% include "../../common/page-header/tpl.twig" with {
				prefix:"page-header__", 
				mod:heading_val_mod,
				level:heading_val_level, 
				heading:heading_val
			} %}
		</div>
	</section>
{% endif %}

{% if view == "leasing_condition" %}
	<section class="{{content_prefix}}panel  {{content_mod}}">
		<div class="{{content_prefix}}container container  {{content_mod}}">	
			<div class="{{content_prefix}}row  grid__row   {{content_mod}}">
				<div>
					{% include "../../common/page-header/tpl.twig" with {
						prefix:"page-header__", 
						mod:heading_mod,
						level:heading_level, 
						heading:heading
					} %}			
					{% include "../../common/page-header/tpl.twig" with {
						prefix:"page-header__", 
						mod:heading_sm_mod,
						level:heading_sm_level, 
						heading:heading_sm
					} %}	
					<div class="{{content_prefix}}row  row  {{content_mod}}-label">
						{% for value in list %}						
						<p class="{{content_prefix}}label  cols  {{value.mod}}  {{content_mod}}">{{value.label}} <span>{{value.value}}</span></li>
						{% endfor %}
					</div>	
				</div>	
				<div>
					{% include "../../common/form/form.twig" with {
						view: 			"leasing_base", 
						prefix:			prefix.form, 
						open:			"true", 
						group:			"is--md-open", 
						mod:			"is--leasing-base", 
						color:			"is--primary-light", 
						ID: 			"f_leasing-base", 
						heading:		"Запрос на лизинг", 
						heading_level:	"h3", 
						heading_mod:	"is--h3  fw--700  is--form-leasing-base",
						heading_sm:		"Оставьте заявку, и наш консультант перезвонит!", 
						heading_sm_level:"p", 
						heading_sm_mod:	"is--h6  fw--100  is--form-leasing-base-xs",
						heading_r:		"Спасибо!<br> Заявка отправлена", 
						heading_r_level:"h3", 
						heading_r_mod:	"is--h2 fw--700  is--form-leasing-base-result",
						heading_r_sm:	"В ближайшее время наш консультант перезвонит вам и ответит на вопросы.", 
						heading_r_sm_level:	"p", 
						heading_r_sm_mod:	"is--h6  fw--100"
					} %}	
				</div>	
			</div>
		</div>
	</section>
{% endif %}

{% if view == "leasing_formaliz" %}
	<section class="{{content_prefix}}panel  {{content_mod}}">
		<div class="{{content_prefix}}container container  {{content_mod}}">
			{% include "../../common/page-header/tpl.twig" with {
				prefix:"page-header__", 
				mod:heading_mod,
				level:heading_level, 
				heading:heading
			} %}		
			<div class="{{content_prefix}}row  grid__row   {{content_mod}}">
				<div>				
					{% include "../../common/tabs/tpl.twig" with {
						view:"leasing_formaliz",
						tabs:"leasing_", 
						prefix:prefix.tabs,
						mod:"is--default",
						group:content_mod,
						list:list
					} %}
				</div>
				<picture>
					<img src="{{path.img}}/default/leasing-formaliz.svg" class="img-responsive" alt="">
				</picture>
			</div>
		</div>
	</section>
{% endif %}

{% if view == "leasing_adv" %}
	<section class="{{content_prefix}}panel  {{content_mod}}">
		<div class="{{content_prefix}}container container  {{content_mod}}">
			{% include "../../common/page-header/tpl.twig" with {
				prefix:"page-header__", 
				mod:heading_mod,
				level:heading_level, 
				heading:heading
			} %}		
			<div class="{{content_prefix}}slider   {{content_mod}}" data-slick-leasing-adv>				
				{% for value in list %}
				<div class="item  {{value.status}}">
					<div class="{{prefix.card}}item  {{content_mod}}">
						<div class="{{prefix.card}}heading  {{content_mod}}">
							{{value.name}}
						</div>
						<div class="{{prefix.card}}preview  {{content_mod}}">
							<img src="{{value.preview}}" alt="">
						</div>	
						<div class="{{prefix.card}}icon  {{content_mod}}">
							<img src="{{value.icon}}" alt="" class="img-responsive">
						</div>							
						<div class="{{prefix.card}}note  {{content_mod}}">{{value.note}}</div>
					</div>
				</div>
				{% endfor %}
			</div>
		</div>
	</section>
{% endif %}

