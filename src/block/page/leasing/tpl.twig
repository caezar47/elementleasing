{% if view == "leasing_conditions" %}
	<section class="{{content_prefix}}panel  {{content_mod}}">
		<div class="{{content_prefix}}container container  {{content_mod}}">
			<div class="{{content_prefix}}row  grid__row  {{content_mod}}">
				<div>	
					{% include "../../common/page-header/tpl.twig" with {
						prefix:"page-header__", 
						mod:heading_mod,
						level:heading_level, 
						heading:heading
					} %}
					<ul class="{{prefix.tabs}}nav  is--default  is--column">
						{% for value in list %}
						<li class="{{prefix.tabs}}item  is--default  is--column  {{value.status}}">
							<a class="{{prefix.tabs}}link  is--default" href="#{{tabs}}{{value.id}}" data-toggle="tab">{{value.name}}</a>
						</li>
						{% endfor %}
					</ul>
				</div>
				<div>
					{% for value in list %}
					<div class="{{prefix.tabs}}pane  {{value.status}}" id="{{tabs}}{{value.id}}">
						<div class="text__block  {{content_mod}}">
							{{value.note}}
						</div>
					</div>
					{% endfor %}
				</div>				
			</div>
		</div>
	</section>
{% endif %}

