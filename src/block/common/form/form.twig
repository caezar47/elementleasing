{% if view=="login" %}
	<form action="#" class="{{prefix}}panel  {{mod}}">
		{% include "../../common/page-header/tpl.twig" with {heading_view:heading_view, prefix:"page-header__", mod:heading_mod, group:heading_group, level:heading_level, heading:heading} %}
		<ul class="{{prefix}}tabs  {{mod}}">
			<li class="{{prefix}}tabs-item  {{mod}} active">
				<a class="{{prefix}}tabs-link  {{mod}}" href="#login-clients" data-toggle="tab">Клиенту</a>
			</li> 
			<li class="{{prefix}}tabs-item  {{mod}}"><a class="{{prefix}}tabs-link  {{mod}}"  href="#login-partners" data-toggle="tab">Партнеру</a></li>
		</ul>
		{% include "elem-tpl.twig" with {view: "input", prefix:prefix, mod:mod, color:color, list: [
			{"type":"text", "id":"email", "form_id":ID, "name":"Email", "placeholder":"E-mail"},
			{"type":"password", "id":"passoword", "form_id":ID, "name":"Пароль", "placeholder":"Пароль"}
		]} %}
		<div class="{{prefix}}text  {{mod}}"><a href="#">Забыли пароль?</a></div>
		<div class="{{prefix}}btn  {{mod}}  tabs__pane  active" id="login-clients">
			<button type="submit" class="btn__item  is--success  is--arrow  is--md">Войти в кабинет</button>
			<a href="#" class="btn__item  is--black-29  is--arrow  is--md">Регистрация</a>
		</div>
		<div class="{{prefix}}btn  {{mod}}  tabs__pane" id="login-partners">
			<button type="submit" class="btn__item  is--success  is--arrow  is--md">Войти в кабинет</button>
		</div>
	</form>
{% endif %}
{% if view=="leasing" %}
	<form action="#" class="{{prefix}}panel  {{mod}}">
		<div class="container">
			{% include "../../common/page-header/tpl.twig" with {
				heading_view:heading_view, 
				prefix:"page-header__", 
				mod:heading_mod, 
				group:heading_group, 
				level:heading_level, 
				heading:heading} %}
			{% include "../../common/page-header/tpl.twig" with {
				heading_view:heading_view, 
				prefix:"page-header__", 
				mod:heading_sm_mod, 
				group:heading_sm_group, 
				level:heading_sm_level, 
				heading:heading_sm} %}
			<div class="{{prefix}}row  grid__row  {{mod}}">				
				{% include "elem-tpl.twig" with {view: "input", prefix:prefix, mod:mod, color:color, list: [
					{"type":"text", "id":"fio", "form_id":ID, "name":"fio", "placeholder":"Имя"}
				]} %}	
				{% include "elem-tpl.twig" with {
					view:"select", 
					prefix:prefix, 
					mod:mod, 
					color:color, 
					id:"city", 
					form_id:ID, 
					name:"Город", 
					list: [
						{"name": "Город","status": "selected disabled"},
						{"name": "Пункт1"},
						{"name": "Пункт2"},
						{"name": "Пункт3"}
					] } %}		
				{% include "elem-tpl.twig" with {view: "input", prefix:prefix, mod:mod, color:color, list: [
					{"type":"tel", "id":"tel", "form_id":ID, "name":"Телефон", "placeholder":"Телефон"},
					{"type":"email", "id":"email", "form_id":ID, "name":"Email", "placeholder":"E-mail"}
				]} %}
				<button type="submit" class="btn__item  is--arrow  is--md  is--success">Отправить заявку</button>	
			</div>
		</div>
	</form>
{% endif %}
{% if view=="search" %}
	<form action="#" class="{{prefix}}panel  {{mod}}">
		{% include "elem-tpl.twig" with {view: "input", prefix:prefix, mod:mod, color:color, list: [
			{"type":"text", "id":"email", "form_id":ID, "name":placeholder, "placeholder":placeholder}
		]} %}
		<div class="{{prefix}}btn  {{mod}}">
			<button type="submit" class="btn__item  {{btn_mod}}">{{btn_name}}</button>
		</div>
	</form>
{% endif %}
{% if view=="vacancies" %}
	<form action="#" class="{{prefix}}panel  {{mod}}">
		<div class="{{prefix}}container  container  {{mod}}">
			<div class="{{prefix}}wrap  {{mod}}">
				{% include "../../common/page-header/tpl.twig" with {
					heading_view:heading_view, 
					prefix:"page-header__", 
					mod:heading_mod, 
					group:heading_group, 
					level:heading_level, 
					heading:heading} %}
				{% include "../../common/page-header/tpl.twig" with {
					heading_view:heading_view, 
					prefix:"page-header__", 
					mod:heading_sm_mod, 
					group:heading_sm_group, 
					level:heading_sm_level, 
					heading:heading_sm} %}	

				<div class="{{prefix}}row  grid__row  {{mod}}">
					{% include "elem-tpl.twig" with {view: "input", prefix:prefix, mod:mod, color:color, list: [
						{"type":"text", "id":"fio", "form_id":ID, "name":"fio", "placeholder":"Фамилия и Имя"},
						{view: "date","type":"text", "id":"bithday", "form_id":ID, "name":"bithday", "placeholder":"Дата рождения"},
						{"type":"text", "id":"city", "form_id":ID, "name":"city", "placeholder":"Город планируемой работы"},
						{"type":"email", "id":"email", "form_id":ID, "name":"Email", "placeholder":"E-mail"},
						{"type":"tel", "id":"tel", "form_id":ID, "name":"Телефон", "placeholder":"Телефон"}
					]} %}
					{# <div class="{{prefix}}drag  {{color}}  {{mod}}">Перетащите или загрузите сюда резюме</div> #}
					{% include "elem-tpl.twig" with {view:"file", prefix:prefix, mod:mod, color:color, id:"file", form_id:ID, name:"Загрузите сюда резюме" } %}
					{% include "elem-tpl.twig" with {view: "textarea", prefix:prefix, mod:mod, color:color, id:"comments", "form_id":ID, name:"Расскажите, почему вы хотите у нас работать?", placeholder:"Расскажите, почему вы хотите у нас работать?"} %}	
						
					{% include "elem-tpl.twig" with {view:"checkbox", prefix:prefix, mod:"is--sm", color:color, list: [{"id":"agreement", "form_id":ID, "status":"checked", "name":"Согласие с условиями передачи информации", "label":"Соглашаюсь с условиями передачи информации"}] } %}
					<button type="submit" data-test class="btn__item  is--success">Отправить</button>
				</div>
			</div>
			<div class="{{prefix}}result  {{mod}}">
				{% include "../../common/page-header/tpl.twig" with {
					heading_view:heading_view, 
					prefix:"page-header__", 
					mod:heading_r_mod, 
					group:heading_r_group, 
					level:heading_r_level, 
					heading:heading_r} %}
				{% include "../../common/page-header/tpl.twig" with {
					heading_view:heading_view, 
					prefix:"page-header__", 
					mod:heading_r_sm_mod, 
					group:heading_r_sm_group, 
					level:heading_r_sm_level, 
					heading:heading_r_sm} %}
				<div class="{{prefix}}result-img  {{mod}}">
					{% include "../svg-sprite/svg.twig" with {icon:"checked"} %}
				</div>
			</div>
		</div>
	</form>
{% endif %}
