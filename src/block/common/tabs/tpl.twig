{% if view == "index_adv" %}
	<div class="{{content_prefix}}row  grid__row  {{content_mod}}">
		<ul class="{{prefix}}nav  {{mod}}  {{content_mod}}">
			{% for value in list %}
			<li class="{{prefix}}item  {{mod}} {{value.status}}">
				<a class="{{prefix}}link  {{mod}}" href="#{{tabs}}{{value.id}}" data-toggle="tab">{{value.name}}</a>
			</li>
			{% endfor %}
		</ul>	
		<div>
			<div class="{{prefix}}controls  {{content_mod}}">
				<button type="button" class="{{prefix}}controls-btn  is--prev  {{mod}}" id="prev"></button>		
				<button type="button" class="{{prefix}}controls-btn  is--next  {{mod}}" id="next"></button>
			</div>			
			<div>		
				{% for value in list %}
				<div class="{{prefix}}pane  {{value.status}}  {{content_mod}}" id="{{tabs}}{{value.id}}">
					<div class="text__block  {{content_mod}}">
						{{value.note}}
					</div>
					{% if value.preview %}
					<div class="{{prefix}}gallery  {{content_mod}}  row  is--cols-3  is--gutter">
						{% for val in value.preview %}
						<div class="cols">
							<img src="{{val.preview}}" class="img-responsive" alt="">
						</div>
						{% endfor %}
					</div>
					{% endif %}
				</div>
				{% endfor %}
			</div>
		</div>
	</div>
{% endif %}
