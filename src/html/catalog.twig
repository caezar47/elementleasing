<!DOCTYPE html>
<html dir="ltr" lang="ru-RU"> 
	<head>	
    	{% include "../block/common/head/head.twig" with {title:"Каталог"}  %}
	</head>
	<body>
    	{% include "../block/common/navbar/navbar.twig" with {navbar_prefix:"navbar__"} %}

		<main class="content-block__panel">
			{% include "../block/page/catalog.twig" with {
				view:"catalog_page", 
				content_prefix:"content-block__", 
				content_mod:"is--page-catalog", 
				heading_group:"", 
				heading_mod:"is--page-default  is--catalog-screen", 
				heading_level:"h1", 
				heading:"Каталог",  
				heading_cat_mod:"is--h3  fw--500  is--catalog-device", 
				heading_cat_level:"p", 
				heading_cat:"Автокраны <sup>36</sup>", 
				bread_mod:"",
				bread:[
					{name:"Главная", link:page.index},
					{name:"Каталог"}
				] 
			} %}

			{% include "../block/page/catalog.twig" with {
				view:"catalog_adv", 
				content_prefix:"content-block__", 
				content_mod:"is--page-catalog-adv",
				heading_mod:"is--h1  fw--500  margin--tiny", 
				heading_level:"h2", 
				heading:"Вас может заинтересовать",
                card_mod:"is--catalog-promo",
                slider_dots:"false"
			} %}
		</main>
		
    	{% include "../block/common/footer/footer.twig" with {footer_prefix:"footer__"} %}

    	{# {% include "../block/common/modals/include.twig"%} #}
    	{% include "../block/common/script/script.twig"%}
	</body>
</html>